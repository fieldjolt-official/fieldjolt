generator cloudflare {
  provider     = "prisma-client-js"
  output       = "../../../../node_modules/.prisma/client-cloudflare"
  engineType   = "client"
  runtime      = "workerd"
  moduleFormat = "esm"
}

generator vercel {
  provider     = "prisma-client-js"
  output       = "../../../../node_modules/.prisma/client-vercel"
  engineType   = "client"
  moduleFormat = "esm"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
